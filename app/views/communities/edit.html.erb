<h2>  Edit Community </h2>

<%= render 'communities/errors' %>

<%= form_with model: @community do |form| %>
    <%= form.label :name %>
    <br>
    <%= form.text_field :name %><br>
    Projects: <%= collection_check_boxes :project, :project_id, Project.all, :id, :name %><br>
    <br> <br>
    <%= form.submit %>
<% end %>